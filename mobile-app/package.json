{
  "name": "researchnow-mobile",
  "version": "1.0.0",
  "description": "ResearchNow Mobile - AI-powered research paper summarization for iOS and Android",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "pods": "cd ios && pod install && cd ..",
    "clean": "react-native clean-project-auto",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace ResearchNow.xcworkspace -scheme ResearchNow -configuration Release"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/native-stack": "^6.9.17",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.1",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.6.5",
    "zustand": "^4.4.7",
    "react-native-paper": "^5.11.6",
    "react-native-vector-icons": "^10.0.3",
    "react-native-svg": "^14.1.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-webview": "^13.6.4",
    "react-native-pdf": "^6.7.4",
    "react-native-share": "^10.0.2",
    "react-native-document-picker": "^9.1.1",
    "react-native-fs": "^2.20.0",
    "react-native-device-info": "^10.12.0",
    "react-native-keychain": "^8.2.0",
    "@react-native-community/netinfo": "^11.1.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-shimmer-placeholder": "^2.0.9",
    "react-native-markdown-display": "^7.0.2",
    "date-fns": "^3.2.0",
    "zod": "^3.22.4",
    "react-hook-form": "^7.49.3",
    "@hookform/resolvers": "^3.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/runtime": "^7.23.8",
    "@react-native/babel-preset": "^0.73.19",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.3",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.48",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.7",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "jest": "^29.7.0",
    "react-test-renderer": "18.2.0",
    "@testing-library/react-native": "^12.4.3",
    "@testing-library/jest-native": "^5.4.3",
    "metro-react-native-babel-preset": "^0.77.0"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-.*|@react-navigation)/)"
    ]
  },
  "keywords": [
    "research",
    "papers",
    "summarization",
    "ai",
    "mobile",
    "ios",
    "android",
    "arxiv",
    "pubmed"
  ],
  "author": "ResearchNow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ResearchNow.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ResearchNow/issues"
  },
  "homepage": "https://researchnow.app"
}
